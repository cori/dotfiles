				<key>script</key>
				<string>import sys

sys.path.append("/Users/cori/code/personal/scripts/")

import ordinalize_date

yesterday = ordinalize_date.ordinalize_date(daysago=1)
lastweek = ordinalize_date.ordinalize_date(weeksago=1)
lastmonth = ordinalize_date.ordinalize_date(weeksago=4)

query_template = "{{{{[[query]]: {{and: {{not: [[query]]}} {{or: #Journal [[Quick Capture]]}} {{between:{0} {0}}} }} }}}}"

yday_query = query_template.format(yesterday)

lastweek_query = query_template.format(lastweek)

lastmonth_query = query_template.format(lastmonth)

all_queries = """ - #Review
    - {}
    - {}
    - {}""".format(yday_query,lastweek_query,lastmonth_query)

sys.stdout.write(all_queries)</string>
